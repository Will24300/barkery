#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/node_modules:/mnt/c/Users/PC/Desktop/node_modules:/mnt/c/Users/PC/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/PC/desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/backend/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/barkery/node_modules:/mnt/c/Users/PC/Desktop/my-git-repos/node_modules:/mnt/c/Users/PC/Desktop/node_modules:/mnt/c/Users/PC/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/PC/desktop/my-git-repos/barkery/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
